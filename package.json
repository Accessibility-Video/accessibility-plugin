    "url": "git+https://github.com/Accessibility-Video/accessibility-plugin.git"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@open-wc/eslint-config": "^7.0.0",
    "@rollup/plugin-commonjs": "^21.0.2",
